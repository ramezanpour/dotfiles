{
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "markdown",
    "go",
    "python",
    "javascriptreact",
    "javascript",
    "css",
    "html"
  ],
  "cSpell.userWords": ["dockerignore", "markdownlint", "signup"],
  "markdownlint.onSave": true,
  "markdownlint.config": {
    "MD013": false
  },
  "prettier.singleQuote": true,
  "go.tags": {
    "transform": "camelcase"
  }
}
